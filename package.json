{
  "name": "sanetools",
  "type": "module",
  "version": "0.1.1",
  "bin": {
    "sanetools": "./bin/sanetools.js"
  },
  "scripts": {
    "initialize": "cp .env.sample .env && npm run db:migrate initial && npm run db:deploy",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "db:migrate": "prisma migrate dev --name",
    "db:deploy": "npm run db:deploy:dev && npm run db:deploy:test",
    "db:deploy:dev": "prisma migrate deploy",
    "db:deploy:test": "dotenv -e .env.test prisma migrate deploy",
    "db:studio": "prisma studio",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "author": "Bruno Pagno",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.6.0",
    "globals": "^15.7.0",
    "prettier": "^3.3.2",
    "prisma": "^5.15.1",
    "supertest": "^7.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.10",
    "typescript-language-server": "^4.3.3",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "express": "^4.19.2"
  }
}
